{
    "name": "@pokimane-safety/app",
    "version": "1.0.0",
    "description": "A React app that powers the stream-overlay.",
    "engines": {
        "node": ">= 16.12.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/PokimaneSafety/pausemane-stream-overlay.git",
        "directory": "packages/app"
    },
    "bugs": {
        "url": "https://github.com/PokimaneSafety/pausemane-stream-overlay/issues"
    },
    "license": "GPL-3.0-or-later",
    "scripts": {
        "start": "./scripts/start.js",
        "kill": "./scripts/kill.sh",
        "build": "./scripts/build.js",
        "build:docker": "docker-compose build pausemane-stream-overlay-app",
        "typecheck": "tsc -p tsconfig.json --noEmit",
        "clean": "tsc -b tsconfig.build.json --clean",
        "postclean": "rimraf build coverage",
        "lint": "eslint './**/*.{js,jsx,ts,tsx}' --ignore-path=../../.eslintignore",
        "lint:fix": "yarn lint --fix",
        "format": "prettier --list-different './**/*.{js,jsx,ts,tsx,json,md,yaml}' --ignore-path ../../.prettierignore",
        "format:fix": "prettier --write './**/*.{js,jsx,ts,tsx,json,md,yaml}' --ignore-path ../../.prettierignore",
        "spelling": "cspell --no-must-find-files --config=../../.cspell.json './**/*.{js,jsx,ts,tsx,md}'",
        "test": "./scripts/test.js --verbose --passWithNoTests",
        "test:watch": "yarn test --watchAll --verbose=false",
        "test:coverage": "yarn test --coverage",
        "test:coverage:watch": "yarn test:coverage  --watchAll --verbose=false"
    },
    "dependencies": {
        "@pokimane-safety/protocol": "^1.0.0"
    },
    "devDependencies": {
        "@babel/core": "7.12.3",
        "@pmmmwh/react-refresh-webpack-plugin": "0.4.3",
        "@testing-library/jest-dom": "^5.15.1",
        "@testing-library/react": "^12.1.2",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.0.3",
        "@types/node": "^16.11.10",
        "@types/react": "^17.0.37",
        "@types/react-dom": "^17.0.11",
        "font-proxima-nova-scss": "^1.0.1",
        "node-sass": "^6.0.1",
        "prettier": "^2.5.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-use-websocket": "^2.9.1",
        "typescript": "^4.5.2",
        "web-vitals": "^2.1.2",
        "@svgr/webpack": "5.5.0",
        "@typescript-eslint/eslint-plugin": "^4.5.0",
        "@typescript-eslint/parser": "^4.5.0",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^26.6.0",
        "babel-loader": "8.1.0",
        "babel-plugin-named-asset-import": "^0.3.7",
        "babel-preset-react-app": "^10.0.0",
        "bfj": "^7.0.2",
        "camelcase": "^6.1.0",
        "case-sensitive-paths-webpack-plugin": "2.3.0",
        "css-loader": "4.3.0",
        "dotenv": "8.2.0",
        "dotenv-expand": "5.1.0",
        "eslint": "^7.11.0",
        "eslint-config-react-app": "^6.0.0",
        "eslint-plugin-flowtype": "^8.0.2",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jest": "^24.1.0",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-react": "^7.21.5",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-plugin-testing-library": "^3.9.2",
        "eslint-webpack-plugin": "^2.5.2",
        "file-loader": "6.1.1",
        "fs-extra": "^9.0.1",
        "html-webpack-plugin": "4.5.0",
        "identity-obj-proxy": "3.0.0",
        "jest": "26.6.0",
        "jest-circus": "26.6.0",
        "jest-resolve": "27.3.1",
        "jest-watch-typeahead": "0.6.1",
        "mini-css-extract-plugin": "0.11.3",
        "optimize-css-assets-webpack-plugin": "5.0.4",
        "pnp-webpack-plugin": "1.6.4",
        "postcss-flexbugs-fixes": "4.2.1",
        "postcss-loader": "3.0.0",
        "postcss-normalize": "8.0.1",
        "postcss-preset-env": "6.7.0",
        "postcss-safe-parser": "5.0.2",
        "prompts": "2.4.0",
        "react-app-polyfill": "^2.0.0",
        "react-dev-utils": "^11.0.3",
        "react-refresh": "^0.8.3",
        "resolve": "1.20.0",
        "resolve-url-loader": "^3.1.2",
        "sass-loader": "^10.0.5",
        "semver": "7.3.2",
        "style-loader": "1.3.0",
        "terser-webpack-plugin": "4.2.3",
        "ts-pnp": "1.2.0",
        "url-loader": "4.1.1",
        "webpack": "4.46.0",
        "webpack-dev-server": "3.11.1",
        "webpack-manifest-plugin": "2.2.0",
        "workbox-webpack-plugin": "5.1.4"
    }
}
